package com.marketkurly.web.domains;

import org.springframework.context.annotation.Lazy;
import org.springframework.stereotype.Component;
import com.marketkurly.web.util.Proxy;
import lombok.Data;


@Data
@Component("pager")
@Lazy
public class Pager extends Proxy{
 private int rowCount, startRow,endRow
 pageSize, pageCount,startPage,endPage,
 nowPage,blockSize,blockCount,preblock,nextBlock;
	
}